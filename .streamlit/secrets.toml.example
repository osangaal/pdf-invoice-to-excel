# Archivo de ejemplo para secrets de Streamlit Cloud
# Copia este archivo como .streamlit/secrets.toml y configura tus API keys

[api_keys]
openai_api_key = "tu_openai_api_key_aqui"
llmwhisperer_api_key = "tu_llmwhisperer_api_key_aqui"

[models]
default_model = "gpt-4o-mini"
temperature = 0.0

[llmwhisperer]
base_url = "https://llmwhisperer-api.us-central.unstract.com/api/v2"
wait_timeout = 60
